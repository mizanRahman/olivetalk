<h1>Notifications</h1>

<table class="table table-bordered table-hover">
  <tr>
    <th>activity_type</th>
    <th>time_sent</th>
  </tr>

<% @notifications.each do |notification| %>
  <tr>

    <td><%= notification.activity_type %></td>
    <td><%= time_ago_in_words(notification.time_sent) %> ago</td>


  </tr>
<% end %>
</table>
